{
  "name": "belljar",
  "description": "A tool for mouse neurohistology",
  "version": "0.0.1-alpha",
  "main": "main.js",
  "repository": "git@github.com:asoronow/belljar.git",
  "author": "Alec <a.soronow@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "compile": "tsc",
    "start": "yarn compile && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "python-shell": "^3.0.1",
    "tar": "^6.1.11"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "electron": "18.1.0",
    "eslint": "^7.32.0",
    "typescript": "^4.6.3"
  },
  "build": {
    "appId": "com.belljar.app",
    "productName": "Bell Jar"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "belljar"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
